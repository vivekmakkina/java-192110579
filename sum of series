import java.util.Scanner;
public class SumOfSeries{
   public static void main(String[] args){
      int num,sum=0;
      System.out.println("Enter a num : ");
      Scanner sc = new Scanner(System.in);
      num = sc.nextInt();
      for(int i=0;i<=num;i++)
      {
          sum=sum+i;
      }
      System.out.print("Sum is:"+ sum);
   }
}
